{
  "name": "sharetribe-community-cli",
  "version": "1.16.9",
  "description": "UNOFFICIAL Sharetribe CLI reimplementation built on sharetribe-flex-build-sdk - 100% compatible with official flex-cli",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "sharetribe-community-cli": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "dev": "node build.js --dev --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "postinstall": "printf '\\n\\033[33m⚠️  NOTICE: This is an UNOFFICIAL Sharetribe CLI (community reimplementation).\\033[0m\\n\\033[33m   For the official CLI, install: \\033[36mnpm install -g flex-cli\\033[0m\\n\\n' >&2",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sharetribe",
    "flex",
    "cli",
    "marketplace",
    "transaction-process",
    "unofficial",
    "community"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharetribe-community/sharetribe-cli",
    "directory": "packages/sharetribe-cli"
  },
  "homepage": "https://github.com/sharetribe-community/sharetribe-cli/tree/master/packages/sharetribe-cli",
  "dependencies": {
    "@types/yargs": "^17.0.35",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.23",
    "jsedn": "^0.4.1",
    "sharetribe-flex-build-sdk": "^1.16.9",
    "yargs": "^18.0.0",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.17.10",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
